{{>nav}}
{{#if error}}
<div class="row">
    <div class="col s6 offset-s3">
        <h3 class="red-text text-darken-4 center-align">{{error}}</h3>
    </div>
</div>
{{/if}}
<h1 class="center-align green-text text-darken-4">{{city}}</h1>

{{#if weatherList}}
<ul class="forecast-list">
    {{#each weatherList}}
    {{>forecast}}
    {{/each}}
</ul>
{{else}}
<p>nothing</p>
{{/if}}